@{
    ViewBag.Title = "Point Dashboard";
    ViewBag.Styles = "/css/newCustomer.css";
    ViewBag.TitleSession = "Cadastro de clientes";
    ViewBag.Script = "/js/newCustomer.js";
    Layout = "/Views/Shared/_layoutDashboard.cshtml";
}

<div id="table">
    <form id="formCadastro" asp-controller="Usuario" asp-action="Cadastro" method="POST">
        <div id="consumerInfo">
            <div class="formLine">
                <input id="customerName" class="formInput" type="text" placeholder="Nome">
                <input id="customerCPF" class="formInput" type="text" placeholder="CPF">
            </div>
            <div class="formLine">
                <input id="customerEmail" class="formInput" type="text" placeholder="E-mail">
                <input id="customerPhoneNumber" class="formInput" type="text" placeholder="Telefone">
            </div>
            <div class="formLine">
                <input id="CustomerPassword" class="formInput" type="text" placeholder="Senha">
                <input id="CustomerConfirmPassword" class="formInput" type="text" placeholder="Confirmar Senha">
            </div>
        </div>
        <hr class="divForm">
        <div id="adresses">

        </div>
        <div id="formOptionsBtn">
            <div id="newAdress">
                <span class="btnForm" onclick="newAdress()" >Adicionar Endere√ßo +</span>
            </div>
            <button id="btnSubmit" class="btnForm" type="submit">Salvar <i class="far fa-save"></i></button>
        </div>
    </form>
</div>